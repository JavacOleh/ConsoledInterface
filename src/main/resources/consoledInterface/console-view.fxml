<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.TextFlow?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="385.0" prefWidth="640.0" VBox.vgrow="ALWAYS">
      <children>
            <Button fx:id="enterButton" layoutX="522.0" layoutY="361.0" mnemonicParsing="false" onMouseClicked="#onEnterButtonClick" prefHeight="25.0" prefWidth="52.0" text="Enter" />
            <TextField fx:id="enterTextField" layoutX="3.0" layoutY="361.0" onKeyPressed="#onEnterKeyPress" prefHeight="25.0" prefWidth="515.0" />
            <ScrollPane fx:id="consoleScrollPane" layoutX="3.0" layoutY="5.0" prefHeight="353.0" prefWidth="634.0">
               <content>
                  <StackPane fx:id="consoleStackPane" prefHeight="351.0" prefWidth="629.0">
                     <children>
                        <TextFlow fx:id="textArrayInside" prefHeight="200.0" prefWidth="200.0" />
                     </children></StackPane>
               </content>
            </ScrollPane>
            <Button fx:id="clearButton" layoutX="580.0" layoutY="361.0" mnemonicParsing="false" onMouseClicked="#onClearButtonClick" prefHeight="25.0" prefWidth="52.0" text="Clear" />
      </children>
    </AnchorPane>
  </children>
</VBox>
